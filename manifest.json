{
    "manifest_version": 3,
    "name": "GiphMe",
    "version": "1.1",
    "description": "Allow to download some giph directly with one button",
    "content_scripts": [
      {
        "js": ["scripts/content.js"],
        "matches": [
          "https://tenor.com/**",
          "http://tenor.com/**"
        ]
      },
      {
        "css": ["styles/main.css"],
        "matches": [
            "https://tenor.com/**",
          "http://tenor.com/**"
          ]
      }
    ]
  }